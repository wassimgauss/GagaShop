{% set i =0 %}
{% for category in listCategory %}
    {% if category.id != 5 %}
    <li>
        <a href="{{ path('shoppage_category', { 'nom_categ' : category.nomUrl }) }}">{{ category.nom }}
            <span class="count">({{ count[i] }})</span></a>
    </li>
    {% set i = i+1 %}
    {% endif %}
{% endfor %}