         */

        body {
            background: url('{{ asset('build/img/poly-bg.jpg') }}') no-repeat center center fixed;
            -webkit-background-size: cover;
            -moz-background-size: cover;
            background-size: cover;
            -o-background-size: cover;
            width:100%;
            height:100%;
            background-size: calc(100% + 50px);
        }
        .btn {
            border-radius: 25px;
        }
        .boxy {
            padding: 0px 0px 0px;
        //background: #FAFAFA;
            background: rgba(255,255,255,1);
        }
        .effect3
        {
            position: relative;
        }
        .effect3:before
        {
            z-index: -1;
            position: absolute;
            content: "";
            bottom: 15px;
            left: 10px;
            width: 50%;
            top: 80%;
            max-width:300px;
            background: #777;
            -webkit-box-shadow: 0 15px 10px #777;
            -moz-box-shadow: 0 15px 10px #777;
            box-shadow: 0 15px 10px #777;
            -webkit-transform: rotate(-3deg);
            -moz-transform: rotate(-3deg);
            -o-transform: rotate(-3deg);
            -ms-transform: rotate(-3deg);
            transform: rotate(-3deg);
        }
        .navbar {
            border: none;
            -webkit-transition: 0.2s;
            transition: 0.2s;
        }
        .navbar--size--xl-stick {
            padding: 1.2rem 0;

        }
        .navbar--size--md {
            padding: 1rem 0;
        }
        .navbar--color--white {
            background: #fff;
        }
        .navbar--color--ghost {
            /* background: rgba(53, 59, 72,0.5);*/
            /* background: rgba(47, 54, 64,0.4);*/
            background:
                    -webkit-linear-gradient(left,#F2F3F5 1%,#F2F3F5 40%,#F2F3F5 52%,rgba(242,243,245,.63) 90%);
            margin-bottom: 0px;
            border-bottom: inherit !important;
            border-right: inherit !important;
        }
        .navbar--color--ghost a {
        //color: white;
        //border-left: inherit !important;

        }
        .navbar--color--ghost .navbar-brand,
        .navbar--color--ghost .navbar-text,
        .navbar--color--ghost li a{
            /*color:white;*/
        }
        /*                .sub-navbar ul li{
                            border-right:1px solid #83939C;
                            border-bottom:1px solid #83939C;
                        }*/
        .nav-sticky {
            border-right: inherit !important;

        }
        .gs {
            box-shadow: 0 14px 28px rgba(0,0,0,0.25), 0 10px 10px rgba(0,0,0,0.22);
        }
    </style>
</head>
<body>
{% block body %}
    <div class="container">
    <div class="row">
    <div class="boxy">
    {% block navbar %}


                {{ include ('@Gauss/Default/layout/navbar.html.twig') }}

    {% endblock %}

    {% block slider %} {% endblock %}

    {% block collection %} {% endblock %}
    {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
        {{ 'layout.logged_in_as'|trans({'%username%': app.user.username}, 'FOSUserBundle') }} |
        <a href="{{ path('fos_user_security_logout') }}" style="opacity: 0">
            {{ 'layout.logout'|trans({}, 'FOSUserBundle') }}
        </a>
    {% else %}
        <a href="{{ path('fos_user_security_login') }}" style="opacity: 0">{{ 'layout.login'|trans({}, 'FOSUserBundle') }}</a>
    {% endif %}

    {% if app.request.hasPreviousSession %}
        {% for type, messages in app.session.flashbag.all() %}
            {% for message in messages %}
                <div class="flash-{{ type }}" style="opacity: 0">
                   {# {{ message }}#}
                </div>
            {% endfor %}
        {% endfor %}
    {% endif %}

    {% block fos_user_content %}

    {% endblock fos_user_content %}


    {% block promotion %} {% endblock %}

    {% block content %} {% endblock %}

    {% block brands %} {% endblock %}




    {% block footer %}
        <div style="margin-top: 0px;" class="footer">
            {{ include ('@Gauss/Default/layout/footer-menu.html.twig') }}
        </div>
    {% endblock %}

    <!-- open/close -->
    <div class="overlay overlay-simplegenie">
        <i class="overlay-close lil-close"></i>
        <div class="container">
            <div class="row text-center pos-r">
                <div class="col-md-7 col-xs-11 col-center col-height-center search-form">
                    <form action="#!" method="POST" class="inputs-bg" id="searchform">
                        <strong>Type a sentence or word you want to search for, And press Enter.</strong>
                        <input type="text" name="q" class="form-control" placeholder="Search..">
                    </form>
                </div>
            </div>
        </div>
    </div><!-- /.overlay -->
          <!-- Go to top -->
    <a href="#top" class="go-to-top">
        <i class="lil-angle-double-down"></i>
    </a>

    </div>
    </div>
    </div>
{% endblock %}
{% block javascripts %}
    <script type="text/javascript" src="{{ asset('build/js/min.js') }}"></script>
{% endblock %}
</body>
</html>

